{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "luiz-dk-sveltekit",
	"compatibility_date": "2026-01-02",
	"compatibility_flags": ["nodejs_als"],
	"pages_build_output_dir": ".svelte-kit/cloudflare",
	"observability": {
		"enabled": true
	},
	"durable_objects": {
		"bindings": [
			{
				"name": "GLOBAL_COUNTER_DO",
				"class_name": "GlobalCounterDurableObject",
				"script_name": "durable-object-global-counter"
			}
		]
	}
}
